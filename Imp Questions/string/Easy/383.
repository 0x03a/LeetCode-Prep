// optimal using own hash
// TC O(n)

vector<int> m1 (26,0);
for(int i=0; i< magazine.length();i++)
     {
        m1[ magazine[i]-'a']++;
     }

     for(int i=0; i< ransomNote.length();i++)
     {
        m1[ransomNote[i]-'a']--;
     }

     for(int i=0; i<26;i++)
     {
        if(m1[i] <0)
          return false;
     }

     return 1;


     

}



